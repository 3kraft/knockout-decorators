{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "suppressImplicitAnyIndexErrors": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": ["dom", "es5", "es2015.symbol"],
    "target": "es5",
    "plugins": [{
      "name": "tslint-language-service"
    }]
  }
}